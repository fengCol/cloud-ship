<div class="wrap container">
    <div class="container main">
        <div class="row">
            <ul class="finance-ul">
                <span>类别</span>
                <li ng-repeat="list in vm.sliceParentList"
                    ng-click="vm.goAnchor(list.id)">
                    {{list.name}}</li>
                <li class="expect">（所有服务暂时仅支持北京地区）</li>
            </ul>
        </div>
    </div>
</div>
<div class="container"
      ng-class="{tianshi:$index == 1}">
    <div class="text-center">
        <h3     id="{{vm.sliceParentList[1].id}}"
                class="angelInvestor angelTitle">
            <i class="line-l"></i><span>{{ vm.sliceParentList[1].name}}</span><i class="line-r"></i>
        </h3>
    </div>
    <div class="hundredConpany">
        <span>共计<span class="yellow">{{vm.data2.totaltianshi}}</span>家</span>
        <span class="pull-right checkmore"
              ui-sref="app.link({id: tid,pid:tid})">
            更多></span>
    </div>
    <div class="col-lg-3 col-md-6 col-sm-12 hellongiht"
         ui-sref="app.details({cid:oneList.angelServerList[0].cid,sid:oneList.angelServerList[0].sid,sname:vm.sliceParentList[1].name})"
         ng-repeat="oneList in  vm.cList | limitTo: 8">
        <!--{{oneList.angelServerList[$index].sid}}-->
        <div class="companyTag">
            <div class="tagLogo" style="background: url({{oneList.logo}})   center no-repeat;background-size: 60% ">
                <div class="tagLevel">
                    <span ng-mouseenter="levelShow=true" ng-mouseleave="levelShow=false">L{{oneList.grade}}级</span>
                </div>
            </div>

            <div class="tagText">
                <h4>{{oneList.cname}}</h4>
                <p>{{oneList.profile}}</p>
                <span class="yellowSpan">询价</span>
                <span class="ooSpan">{{oneList.angelServerList[0].countNum}}</span>
                <span class="moreThan" ng-if="comList.count > 100">＋</span>

            </div>
            <!--<div class="levelCont" ng-if="levelShow">-->
                <!--<p>L{{oneList.grade}}级合作伙伴:</p>-->
                <!--<p>{{oneList.gradeCon}}</p>-->
            <!--</div>-->
        </div>
    </div>
</div>

<div class="container"
     ng-class="{fengxian:$index == 1}">
    <div class="text-center">
        <h3     id="{{vm.sliceParentList[0].id}}"
                class="angelInvestor angelTitle">
            <i class="line-l"></i><span>{{ vm.sliceParentList[0].name}}</span><i class="line-r"></i>
        </h3>
    </div>
    <div class="hundredConpany">
        <span>共计<span class="yellow">{{vm.data3.totalfenxian}}</span>家</span>
        <span class="pull-right checkmore"
              ui-sref="app.link({id:fid,pid:fid})">
            更多></span>
    </div>
    <div class="col-lg-3 col-md-6 col-sm-12 hellongiht" ng-click="vm.goDetails"
         ui-sref="app.details({cid:twoList.investServerList[0].cid,sid:twoList.investServerList[0].sid,sname:vm.sliceParentList[0].name})"
         ng-repeat="twoList in  vm.cfList | limitTo: 8" >
        <div class="companyTag">
            <div class="tagLogo" style="background: url({{twoList.logo}})   center no-repeat;background-size: 60% ">
                <div class="tagLevel">
                    <span ng-mouseenter="levelShow=true" ng-mouseleave="levelShow=false">L{{twoList.grade}}级</span>
                </div>
            </div>
            <div class="tagText">
                <h4>{{twoList.cname}}</h4>
                <p>{{twoList.profile}}</p>
                <span class="yellowSpan">询价</span>
                <span class="ooSpan">{{twoList.investServerList[0].countNum}}</span>
                <span class="moreThan" ng-if="comList.count > 100">＋</span>

            </div>
            <div class="levelCont" ng-if="levelShow">
                <p>L{{twoList.grade}}级合作伙伴:</p>
                <p>{{twoList.gradeCon}}</p>
            </div>
        </div>
    </div>
</div>
