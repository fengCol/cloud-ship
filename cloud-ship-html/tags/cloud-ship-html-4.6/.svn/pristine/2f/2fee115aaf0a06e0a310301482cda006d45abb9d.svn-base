.jqcarouse-style2 .carousel .carousel-control.left {
    opacity: 1;
    left: 0px;
    width: 60px;
    text-align: left;
}

.jqcarouse-style2 .carousel .carousel-control.left span img {
    opacity: 0.5;
}

.jqcarouse-style2 .carousel .carousel-control.left:hover span img {
    opacity: 0.9;
}

/*.jqcarouse-style2 .carousel .carousel-control.left:before {*/
/*content: '\e079';*/
/*}*/

.jqcarouse-style2 .carousel .carousel-control.left:after {
    right: -100%;
}

.jqcarouse-style2 .carousel .carousel-control.right {
    opacity: 1;
    right: 0px;
    width: 60px;
    text-align: right;
}

.jqcarouse-style2 .carousel .carousel-control.right span img {
    opacity: 0.5;
}

.jqcarouse-style2 .carousel .carousel-control.right:hover span img {
    opacity: 0.9;
}

