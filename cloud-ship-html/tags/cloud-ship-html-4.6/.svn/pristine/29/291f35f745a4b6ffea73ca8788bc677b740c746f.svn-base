    margin: 10px 0 0 10px;
}
/*卡片*/
.hellongiht{
    padding: 15px;
    margin: 0 auto;
    -webkit-transition: 1s;
    -moz-transition: 1s;
    transition: 1s;
}
/*动画*/
.hellongiht.ng-enter{opacity: 0;}
.hellongiht.ng-enter-active{opacity: 1;}
.companyTag{
    position: relative;
    width: 255px;
    margin: 0 auto;
    border-radius: 5px;
    border: 1px #e8e8e8 solid
    
}
.companyTag:hover{
    cursor: pointer;
    box-shadow: 0 0 10px #e8e8e8 ;
}
.tagLogo{
    overflow: hidden;
    height: 160px;
    border-bottom: 1px solid #e8e8e8;
    /*background: url("../images/thirdpService/linkPage/background.png")*/
                /*#3e9cfe*/
                /*center*/
                /*no-repeat;*/
}
.tagLevel {
    margin-top: 10px;
    background: url("../images/thirdpService/linkPage/diamond.png")
                no-repeat
                190px
                center;
}
.tagLevel span {
    margin-left: 190px;
    padding-left: 20px;
    padding-right: 10px;
    color: #fff;
    font-size: 12px;
    vertical-align: text-top;
    cursor: default;
}
.levelCont {
    position: absolute;
    padding: 5px;
    width: 100px;
    height:100px;
    font-size: 12px;
    border: 1px solid #e8e8e8;
    background-color: #fff;
    top: 18px;
    left: 237px;
    z-index: 999;
    -webkit-transition: 1s all;
    -moz-transition: 1s all;
    transition: 1s all;
}
/*显示操作*/
.levelCont.ng-enter{opacity: 0;}
.levelCont.ng-enter-active{opacity: 1;}
/*隐藏操作*/
.levelCont.ng-leave{opacity: 1;}
.levelCont.ng-leave-active{opacity: 0;}
.tagText{
    font-size: 12px;
    color: rgb(153, 153, 153);
    line-height: 18px;
    background-color: #ffffff;
    padding: 20px 14px 13px 13px;
}
/*查看更多*/
.checkmore:hover {
    cursor: pointer;
    color: #4ea4fe;
}
/*申请数后面加号*/
.moreThan {
    padding: 0 !important;
}
.tagText h4 {
