}



.tagText p{
    height: 18px;
    margin-bottom: 5px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}



.tagText span{
    padding-left: 18px;
    background: left center ;
}
.tagText .yellowSpan{
    background: no-repeat url("../images/thirdpService/linkPage/service.png");
    color: #999;
}
.tagText .yellowSpan:hover{
