}
/*登录注册模态框*/
.loginVerifyBtn{
    background-color: #4ea4fe;
    color: white;
}

/*登录模态框开始*/
.modal-xs {
    min-width:300px;
    width: 300px;
}
.modal-header{
    border: 0;
}
.modal-header .close{
    outline: none;
    position: absolute;
    top: 0;
    right: 30px;
    font-size: 22px;
    color: #000;
    width: 27px;
    height: 27px;
    margin: 0;
    background-color: #dbdedf;
    border-bottom-right-radius: 50%;
    border-bottom-left-radius: 50%;
    box-shadow: 0 0 1px #e0e5ea;
    opacity: .5;
}
.modal-header > h5{
    display: inline-block;
    margin-left: 15px;
    margin-right: 15px;
    cursor: pointer;
    color: #999;
    text-align: center;
    width: 70px;
    line-height: 40px;
    border-bottom: 2px solid transparent;
}
h5.modalAction{
    color: #4ea4fe;
    border-bottom: 2px solid #4ea4fe;
}
.phone-psd {
    margin: 0 15px;
}
.warning-tips-red{
    margin: 0 15px;
    font-size: 10px;
    color: red;
}
.send-code {
    margin-top: 15px;
}
.keeps {
    margin: 0 15px;
}
.forget-psd {
    display: inline-block;
    position: relative;
    left:100px;
}
.keeps span,
.keeps span>a{
    cursor: pointer;
    font-weight: normal;
    color: #666;
    font-size: 12px;
}
.my-login {
    width: 240px;
    margin: 0 auto;
}
.my-login-form {
    margin-bottom: 10px;
}
.modal-footer{
    padding-left: 0;
    padding-right: 0;
}
.modal-xs .form-inline{
    margin-bottom: 15px;
}
.modal-xs .message{
    border: 0;
    color: #fff;
    margin-left: 12px;
    background-color: #4ea4fe;
}
.modal-xs .btn-warning{
    background-color: #ff971c;
}
/*定义input高度*/
.modal-xs input[type='text'],
.modal-xs input[type='password'],
.modal-xs .btn{
    height: 40px;
}

.modal-xs input,
.modal-xs button,
.modal-xs .input-group-addon{
    border-radius: 0;
}



.change-title{
    font-size:14px;
    color:#666666;
}

