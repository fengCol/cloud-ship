/*图片上传*/
.uploader-box {
    position: relative;
    width: 130px;
    height: 130px;
    line-height: 130px;
    overflow: hidden;
    border: 1px solid #e8e8e8;
    background-color: #fff;
    /*-webkit-border-radius: 50%;*/
    /*-moz-border-radius: 50%;*/
    /*border-radius: 50%;*/
    background-image:url(../images/user/empty-logo2.png);
    background-position:50% 50%;
    background-repeat: no-repeat;
    background-size: 100%;
}
.uploader-div {
    display: inline-block;
    position: relative;
    width: 50%;
    height: 260px;
    overflow: hidden;
    border: 1px solid #e8e8e8;
    line-height: 260px;
    text-align: center;
    background-image: url(../images/user/empty-logo2.png);
    background-position:50% 50%;
    background-repeat: no-repeat;
    background-size: 50%;
}
.uploader-com {
    display: inline-block;
    position: relative;
    width: 40%;
    height: 50px;
    overflow: hidden;
    border: 1px solid #e8e8e8;
    background-color: #fff;
}
.uploader-com::after,
.uploader-div::after,
.uploader-box::after {
    content: '';
    display: block;
    margin-top: 100%;
}
.uploader-box-img {
    width: 130px;
    height: 130px;
    overflow: hidden;
    background-size: cover;
    background-position: center;
    background-color: #fff;
    /*-webkit-border-radius: 50%;*/
    /*-moz-border-radius: 50%;*/
    /*border-radius: 50%;*/
}
.bgRed{
    background-color: red;
}
.uploader-com p, .uploader-com span

 {
    position: absolute;
    margin: 0;
    padding-top: 45%;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    display: none;
    background: rgba(255,255,255,0.3);
    text-align: center;
}
.uploader-div span,
.uploader-div p,
.uploader-box p,
.uploader-box span{
    position: absolute;
    margin: 0;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    display: none;
    background: rgba(255,255,255,0.3);
    text-align: center;
}
.uploader-com:hover p,
.uploader-div:hover p,
.uploader-box:hover p {
    display: block;
}
.uploader-com span,
.uploader-div span,
.uploader-box span {
    display: block;
}
.uploader-com img,
.uploader-box img {
    width: 100%;
    background-color: #fff;
}
.uploader-div img{
    width: 60%;
    background-color: #fff;
}
.uploader-box-box {
    width: 130px;
    height: 130px;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
}
