html,
body,
div,
ul,
li,
img,
a,
h1,
h2,
h3,
h4,
h5,
p,
span {
    padding: 0;
    margin: 0;
}

body {
    font-size: 62.5%;
    font-family: "Microsoft YaHei", sans-serif;!important;
}

li {
    list-style: none;
}

button,input{
    outline:none;
}

a,
a:hover,
a:active {
    text-decoration: none;
    outline: none;
}

.pointer{
    cursor: pointer;
}

.margin-right {
    margin-right:5px;
}

/*start header*/
header {
    position: relative;
    width: 100%;
    color: #fff;
    background-color: #4ea4fe!important;
}
/*hearer top*/
#header-top{
    font-size: 14px;
    line-height: 40px;
    color: #999999;
    background-color: #fff;
}
#header-top a,
#header-top a:focus,
#header-top a:hover,
#header-top a:valid{
    color: #999999;
    text-decoration:none
}
.message-i{
    display: inline-block;
    width: 10px;
    height: 10px;
    background-color: #f00;
    border-radius: 50%;
    /*vertical-align: middle;*/
}
/*nav*/
nav{
    background-color: #4ea4fe!important;
}
.navbar-brand > img{
    width: 100%;
    max-width: 175px;
}
.list-inline > li{
    padding: 0;
    text-align: center;
    cursor: pointer;
}
.navbar-default .navbar-nav > li > a{
    color: #fff;
    padding:24px 0 28px 0 ;
    min-width: 62px;
    margin-left: 20px;
    text-align: center;
    font-size: 18px;
    border-top: 4px solid #4ea4fe;
    box-sizing: border-box;
}
.navbar-default{
    margin-bottom: 0;
    border: none;
}
.navbar-toggle{
    border: none;
    margin: 0;
    padding: 8px;
}
.navbar-toggle:hover, 
.navbar-toggle:focus{
    background-color: transparent !important;
}
.navbar-default .navbar-nav .active,
.navbar-default .navbar-nav > li > a:hover{
    border-top: 4px solid #fff;
    color: #fff;
}
.navbar-default .navbar-nav .actived {
    border-top: 4px solid #fff;
    color: #fff;
}
.navbar-default .navbar-nav .moreactived {
    border-top: 4px solid #fff;
    color: #fff;
}
.navbar-default .navbar-nav > li > a:visited{
    color: #fff;
}
.flip {
    font-size: 30px;
    padding: 15px;
    cursor: pointer;
}
@media screen  and (max-width: 767px){
    .navbar-default .navbar-nav > li > a{
        margin-left: 0;
    }
}

#logo{
    display: inline-block;
    width: auto;
    max-width: 175px;
    margin-right: 25px;
}

#slogan{
    display: inline-block;
    width: auto;
}

@media (max-width:992px) {
    #slogan{
        display: none;
    }
    #logo{
        margin-right: 0;
    }
}
/*header over*/


/*footer start*/
footer {
    padding-top: 50px;
    color: #999999;
    font-size: 1.4rem;
    background-color: #181e22;
}
.footer p{
    margin-bottom: 10px;
}
.footer-left h1 {
    display: inline-block;
    padding-bottom: 25px;
    font-size: 14px;
}
.footer-left p{
    margin-bottom: 10px;
}
.footer-title > a {
    font-size: 16px !important;
    color: #ffffff !important;
}
.footer-left h1 > a,
.footer-middle h1 > a{
    text-decoration: none;
    color: #999999;
}
.footer-left h1 > a:hover,
.footer-middle h1 > a:hover {
    text-decoration: none;
    color: #fff;
}
.footer-middle span {
    display: inline-block;
    width: 85px;
    /*padding-right: 2.4rem;*/
}
.footer-middle p{
    margin-bottom: 15px;
}
.footer-right {
    margin-top: -25px;
    text-align: center;
}
.footer-right p {
    padding-top: 10px;
}
@media screen and (max-width: 991px) {
    .footer-right {
        margin-top: 20px;
        text-align: left;
    }
}
/*footer over*/



/*go-top*/
#goToTop {
    position: absolute;
    z-index: 9000;
    right: 1rem;
}

.go-top {
    background-color: #595959;
    color: #fff;
    opacity: 0.7;
}

/*go-top按钮*/
#goToTop {
    position: fixed;
    bottom: 30px;
    margin-left: -150px;
}

#goToTop a {
    width: 108px;
    display: block;
    text-align: center;
    font: 11px/100% Arial, Helvetica, sans-serif;
    text-transform: uppercase;
    text-decoration: none;
    color: #bbb;
    -webkit-transition: 1s;
    -moz-transition: 1s;
    transition: 1s;
}

#goToTop a:hover {
    color: #000;
}
/*带线标题*/

.title_heading{
    display: inline-block;
    margin: 0 30px;
    position: relative;
    font-size: 28px;
    color: #000000;
    line-height: 28px;
}
.title_line{
    position: relative;
    top: -8px;
    display: inline-block;
    background-color: #aacdf1;
    height: 1px;
    width: 8%;
}

/* 会员中心标题 */
.user-title {
    padding: 17px 0 17px 20px;
    font-size: 18px;
    font-weight: bold;
    border-bottom: 1px solid #e8e8e8;
}

/*密码修改wrap*/
.user-pwd-wrap{
